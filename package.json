{
	"name": "scryfall-api",
	"version": "3.2.2",
	"description": "A Javascript library for https://scryfall.com/docs/api written in Typescript.",
	"keywords": [
		"magic the gathering",
		"magic",
		"gathering",
		"api",
		"mtg",
		"typescript",
		"card",
		"cards",
		"scryfall",
		"scry"
	],
	"homepage": "https://github.com/MarioMH8/scryfall-api",
	"bugs": {
		"url": "https://github.com/MarioMH8/scryfall-api/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MarioMH8/scryfall-api"
	},
	"license": "MIT",
	"files": [
		"dist"
	],
	"module": "./dist/scryfall-api.mjs",
	"types": "./dist/types/src/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/types/src/index.d.ts",
			"require": "./dist/scryfall-api.js",
			"import": "./dist/scryfall-api.mjs",
			"default": "./dist/scryfall-api.mjs"
		}
	},
	"scripts": {
		"build": "vite build",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"publish": "yarn changeset publish",
		"test": "vitest run --no-isolate",
		"test:coverage": "vitest run --no-isolate --coverage",
		"typecheck": "tsc --noEmit",
		"version": "yarn changeset version && yarn install --mode update-lockfile"
	},
	"engines": {
		"node": ">=18"
	},
	"peerDependencies": {
		"axios": "^0 || ^1"
	},
	"devDependencies": {
		"@changesets/cli": "2.26.0",
		"@hexatool/eslint-config-typescript": "1.0.1",
		"@hexatool/tsconfig": "18.0.0",
		"@types/node": "18.11.18",
		"@vitest/coverage-c8": "^0.28.0",
		"axios": "1.2.6",
		"eslint": "8.33.0",
		"rollup-plugin-typescript2": "0.34.1",
		"typescript": "4.9.4",
		"vite": "4.0.4",
		"vitest": "0.28.1"
	},
	"lint-staged": {
		"*.{ts,tsx}": "eslint --fix",
		"*": "prettier --ignore-unknown --write"
	},
	"volta": {
		"node": "18.13.0",
		"yarn": "3.3.1"
	},
	"packageManager": "yarn@3.3.1"
}
