{
	"name": "scryfall-api-root",
	"version": "0.0.0",
	"description": "A Javascript library for https://scryfall.com/docs/api written in Typescript.",
	"keywords": [
		"magic the gathering",
		"magic",
		"gathering",
		"api",
		"mtg",
		"typescript",
		"card",
		"cards",
		"scryfall",
		"scry"
	],
	"homepage": "https://github.com/MarioMH8/scryfall-api",
	"bugs": {
		"url": "https://github.com/MarioMH8/scryfall-api/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MarioMH8/scryfall-api"
	},
	"private": true,
	"workspaces": [
		"lib/"
	],
	"license": "MIT",
	"scripts": {
		"build": "preconstruct build",
		"lint": "eslint --ext .ts,.tsx --ignore-path .gitignore .",
		"lint:fix": "eslint --ext .ts,.tsx --ignore-path .gitignore --fix .",
		"postinstall": "preconstruct dev && manypkg check",
		"prepare": "husky install",
		"prettier": "prettier --ignore-unknown --check .",
		"prettier:fix": "prettier --ignore-unknown --write .",
		"release": "yarn preconstruct build && yarn changeset publish",
		"test": "yarn workspaces run test"
	},
	"lint-staged": {
		"*.{ts,tsx}": "eslint --fix",
		"*": "prettier --ignore-unknown --write ."
	},
	"dependencies": {
		"@babel/core": "7.18.6",
		"@babel/preset-typescript": "7.18.6",
		"@changesets/cli": "2.23.0",
		"@commitlint/cli": "17.0.3",
		"@commitlint/config-conventional": "17.0.3",
		"@manypkg/cli": "0.19.1",
		"@preconstruct/cli": "2.1.6",
		"@types/jest": "27.5.2",
		"@types/node": "16.11.42",
		"@typescript-eslint/eslint-plugin": "5.30.3",
		"@typescript-eslint/parser": "5.30.3",
		"eslint": "8.18.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "7.30.1",
		"husky": "8.0.1",
		"jest": "27.5.1",
		"lint-staged": "13.0.3",
		"prettier": "2.7.1",
		"ts-jest": "27.1.5",
		"typescript": "4.7.4"
	},
	"preconstruct": {
		"packages": [
			"lib/"
		],
		"globals": {
			"axios": "axios"
		}
	},
	"packageManager": "yarn@3.2.1"
}
