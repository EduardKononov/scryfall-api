{
	"name": "scryfall-api",
	"version": "3.2.5",
	"description": "A Javascript library for https://scryfall.com/docs/api written in Typescript.",
	"keywords": [
		"magic the gathering",
		"magic",
		"gathering",
		"api",
		"mtg",
		"typescript",
		"card",
		"cards",
		"scryfall",
		"scry"
	],
	"homepage": "https://github.com/MarioMH8/scryfall-api",
	"bugs": {
		"url": "https://github.com/MarioMH8/scryfall-api/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MarioMH8/scryfall-api"
	},
	"license": "MIT",
	"files": [
		"dist"
	],
	"module": "./dist/scryfall-api.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/scryfall-api.js",
			"import": "./dist/scryfall-api.mjs",
			"default": "./dist/scryfall-api.mjs"
		}
	},
	"scripts": {
		"build": "bun build",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"test": "bun test",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@changesets/cli": "2.27.1",
		"@hexatool/eslint-config": "2.3.0",
		"@hexatool/tsconfig": "20.0.1",
		"@types/bun": "1.0.1",
		"eslint": "8.56.0",
		"typescript": "5.3.3"
	}
}
